<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <template id="footer_default2" inherit_id="website.footer_default">
      <xpath expr="//div[@id='footer']" position="attributes">
        <attribute name="style">display:none</attribute>
      </xpath>
    </template>
    <template id="footer_fal_one" inherit_id="website.footer_default">
      <xpath expr="//div[@id='footer']" position="after">
        <div class="container">
          <div align="center" class="p-top-20">
            <div class="center-footer">
              <a href="https://www.facebook.com/Falinwa-301586643530584/?fref=ts">
                <i class="fa fa-facebook social-footer-fal"></i>
              </a>
              <a href="https://www.linkedin.com/company/falinwa">
                <i class="fa fa-linkedin social-footer-fal social-footer-fal-no-margin-right"></i>
              </a>
            </div>
            <div class="p-top-10">Copyright Â© FALINWA Limited</div>
            <div class="clear"></div>
          </div>
        </div>
      </xpath>
    </template>
    <template id="header_logo_falinwa" inherit_id="website.layout_logo_show">
      <xpath expr="//header//a[@class='navbar-brand logo']" position="attributes">
        <attribute name="style">display:none</attribute>
      </xpath>
    </template>
    <template id="header_custom_falinwa" inherit_id="website.layout">
      <xpath expr="//header//div[@class='navbar navbar-default navbar-static-top']" position="attributes">
        <attribute name="style">display:none</attribute>
      </xpath>
    </template>
    <template id="header_custom_falinwa-2" inherit_id="website.layout">
      <xpath expr="//header//div[@class='navbar navbar-default navbar-static-top']" position="after">
        <header>
          <nav class="navbar navbar-default navbar-static-top custom-fixed-00" role="navigation" >
            <div class="container">
              <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                  <div class="fl">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                  </div>
                  <div class="fr menu-toggle">MENU</div>
                  <div class="clear"></div>
                </button>
                <div class="fl submenu-head">
                  <a href="">
                    <img src="/falinwa_website_ext/static/src/img/snippet/logo.png" width="80" height="66" alt="logo Falinwa.com" title="Falinwa.com"/>
                  </a>
                </div>
              </div>
              <t t-if="request.httprequest.path == '/page/homepage'">
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                  <ul class="nav navbar-nav head-color fr">
                    <li>
                      <a class="page-scroll" href="#satu">HOME</a>
                    </li>
                    <li >
                      <a class="page-scroll" href="#dua">ABOUT US</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="#tiga">SERVICES</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="#empat">OUR TEAM</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="#lima">JOBS OFFER</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="#enam">REFERENCES AND PARTNERSHIPS</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="#tujuh">CONTACT US</a>
                    </li>
                  </ul>
                  <div class="clear"></div>
                </div>
              </t>
              <t t-if="request.httprequest.path != '/page/homepage'">
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                  <ul class="nav navbar-nav head-color fr">
                    <li>
                      <a class="page-scroll" href="/page/homepage">HOME</a>
                    </li>
                    <li >
                      <a class="page-scroll" href="/page/homepage#dua">ABOUT US</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="/page/homepage#tiga">SERVICES</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="/page/homepage#empat">OUR TEAM</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="/page/homepage#lima">JOBS OFFER</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="/page/homepage#enam">REFERENCES AND PARTNERSHIPS</a>
                    </li>
                    <li>
                      <a class="page-scroll" href="/page/homepage#tujuh">CONTACT US</a>
                    </li>
                  </ul>
                  <div class="clear"></div>
                </div>
              </t>
            </div>
          </nav>
        </header>
        <section class="snippet_language">
        <div class="fal-language">
          <div class="hover-fal00">
            <a href="/id_ID/page/">
              <img src="/fal_website_blocks/static/src/img/snippet/Indonesia.png" width="24" height="24" alt="language Falinwa.com" title="Falinwa.com"/>
            </a>
          </div>
          <div class="hover-fal00">
            <a href="/fr_FR/page/">
              <img src="/fal_website_blocks/static/src/img/snippet/France.png" width="24" height="24" alt="language Falinwa.com" title="Falinwa.com"/>
            </a>
          </div>
          <div class="hover-fal00">
            <a href="/zh_CN/page/">
              <img src="/fal_website_blocks/static/src/img/snippet/China.png" width="24" height="24" alt="language Falinwa.com" title="Falinwa.com"/>
            </a>
          </div>
          <div class="hover-fal00 none-bottom-fal">
            <a href="/en/page/">
              <img src="/fal_website_blocks/static/src/img/snippet/England.png" width="24" height="24" alt="language Falinwa.com" title="Falinwa.com"/>
            </a>
          </div>
        </div>
      </section>
      </xpath>
    </template>
